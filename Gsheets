  function TODAS_VENDAS() {
  var abas = SpreadsheetApp.getActive();
  abas.setActiveSheet(abas.getSheetByName('MENU23'), true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('JAN'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FEV'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('MAR'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('ABR'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('MAI'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('JUN'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('JUL'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('AGO'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('SET'),true);
  abas.getActiveSheet().showSheet();  
  abas.setActiveSheet(abas.getSheetByName('OUT'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('NOV'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('DEZ'),true);
  abas.getRange("i4").activate();
  abas.getActiveSheet().showSheet();}

  function TODAS_FICHAS() {
