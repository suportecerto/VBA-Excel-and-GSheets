  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FICHA JUL'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FICHA AGO'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FICHA SET'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FICHA OUT'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FICHA NOV'),true);
  abas.getActiveSheet().showSheet();
  abas.setActiveSheet(abas.getSheetByName('FICHA DEZ'),true);
  abas.getActiveSheet().showSheet();
  abas.getRange("i4").activate();
  abas.setActiveSheet(abas.getSheetByName('MENU23'), true);
  abas.getActiveSheet().hideSheet();}

