  function CALC(){
  var calc=SpreadsheetApp.getActive();
  calc.setActiveSheet(calc.getSheetByName('CALC'), true);
  calc.getActiveSheet().showSheet();
  calc.setActiveSheet(calc.getSheetByName('MENU23'), true);
  calc.getActiveSheet().hideSheet();}

  function VAI(){
  var ss=SpreadsheetApp.getActive();
  ss.getRange("G4").activate();}


  



